# Release keystore configuration for Play Store builds
# Copy this file to .release-keystore.properties and fill in your values
# DO NOT commit .release-keystore.properties to git!

# Generate a keystore with:
# keytool -genkeypair -v -storetype PKCS12 -keystore release.keystore -alias release -keyalg RSA -keysize 2048 -validity 10000

storePassword=your_store_password_here
keyAlias=release
keyPassword=your_key_password_here
